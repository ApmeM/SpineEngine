<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>SpineEngine.Content</id>
    <version>1.0.0</version>
    <title>SpineEngine.Content</title>
    <authors>ApmeM</authors>
    <owners>ApmeM</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>SpineEngine.Content is a set of predefined content objects with default values to work with SpineEngine.</description>
    <copyright>Copyright © ApmeM 2019</copyright>
    <dependencies>
      <group targetFramework="netstandard2.0">
      </group>
    </dependencies>
    <repository type="git" url="https://github.com/ApmeM/SpineEngine.git" /> 
  </metadata>
  <files>
    <file src="Content\SpineEngine\**\*.*" target="nugetData\Content\SpineEngine" /> 
    <file src="Content\SpineEngineContent.mgcb" target="nugetData\Content\SpineEngineContent.mgcb" />
    <file src="SpineEngine-Content.targets" target="build\SpineEngine.Content.targets" /> 
  </files>
</package>
